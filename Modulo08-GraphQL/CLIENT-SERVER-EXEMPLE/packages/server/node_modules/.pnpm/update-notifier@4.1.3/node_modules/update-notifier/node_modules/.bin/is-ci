else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/lalin/OneDrive/Documentos/Gama Academy/Modulo08-GraphQL/CLIENT-SERVER-EXEMPLE/packages/server/node_modules/.pnpm/is-ci@2.0.0/node_modules/is-ci/node_modules:/mnt/c/Users/lalin/OneDrive/Documentos/Gama Academy/Modulo08-GraphQL/CLIENT-SERVER-EXEMPLE/packages/server/node_modules/.pnpm/is-ci@2.0.0/node_modules:/mnt/c/Users/lalin/OneDrive/Documentos/Gama Academy/Modulo08-GraphQL/CLIENT-SERVER-EXEMPLE/packages/server/node_modules/.pnpm/node_modules:/mnt/c/Users/lalin/OneDrive/Documentos/Gama Academy/Modulo08-GraphQL/CLIENT-SERVER-EXEMPLE/packages/server/node_modules:/mnt/c/Users/lalin/OneDrive/Documentos/Gama Academy/Modulo08-GraphQL/CLIENT-SERVER-EXEMPLE/packages/node_modules:/mnt/c/Users/lalin/OneDrive/Documentos/Gama Academy/Modulo08-GraphQL/CLIENT-SERVER-EXEMPLE/node_modules:/mnt/c/Users/lalin/OneDrive/Documentos/Gama Academy/Modulo08-GraphQL/node_modules:/mnt/c/Users/lalin/OneDrive/Documentos/Gama Academy/node_modules:/mnt/c/Users/lalin/OneDrive/Documentos/node_modules:/mnt/c/Users/lalin/OneDrive/node_modules:/mnt/c/Users/lalin/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/lalin/OneDrive/Documentos/Gama Academy/Modulo08-GraphQL/CLIENT-SERVER-EXEMPLE/packages/server/node_modules/.pnpm/update-notifier@4.1.3/node_modules/is-ci/node_modules:/mnt/c/Users/lalin/OneDrive/Documentos/Gama Academy/Modulo08-GraphQL/CLIENT-SERVER-EXEMPLE/packages/server/node_modules/.pnpm/update-notifier@4.1.3/node_modules:/mnt/c/Users/lalin/OneDrive/Documentos/Gama Academy/Modulo08-GraphQL/CLIENT-SERVER-EXEMPLE/packages/server/node_modules/.pnpm/update-notifier@4.1.3/node_modules/update-notifier/node_modules"
